"*":
  editor:
    fontSize: 15
    invisibles: {}
    showIndentGuide: true
    fontFamily: "Source Code Pro"
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "wrap-guide"
      "welcome"
      "language-toml"
      "autoflow"
      "dev-live-reload"
      "linter-scss-lint"
    ]
  welcome:
    showOnStartup: false
  linter:
    statusIconPosition: "Right"
    lintOnFly: false
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "tool-bar":
    position: "Left"
    iconSize: "16px"
  "mocha-test-runner":
    env: "test"
  react: {}
  autosave:
    enabled: true
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
    lintOnEdit: false
  "linter-scss-lint": {}
  "merge-conflicts": {}
  "atom-beautify":
    _analyticsUserId: "76311505-940d-45f7-9bed-ab533bac8ef9"
  "tree-view": {}
  "atom-material-ui":
    ui:
      disableAnimations: true
      slimScrollbar: true
      accentColor: "Yellow"
    tabs:
      tabSize: "Small"
    treeView:
      compactTreeView: true
    panels:
      depth: true
    fonts: {}
